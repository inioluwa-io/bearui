{
  "name": "rap-ui",
  "version": "0.0.2",
  "description": "React styled component for easier, and faster user interface development",
  "author": "Inioluwa Sogelola <inioluwwa4.is@gmail.com>",
  "homepage": "https://github.com/nerdi-caveman/react-admin-panel#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "esm/index.js",
  "sideEffects": false,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "*.md",
    "esm",
    "src",
    "docs"
  ],
  "devDependencies": {
    "rimraf": "^2.6.3",
    "ts-loader": "^8.0.0",
    "typescript": "^3.9.6"
  },
  "scripts": {
    "build": "yarn run build-cjs && yarn run build-esm",
    "build-cjs": "rimraf ./lib && tsc",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
    "watch": "tsc --outDir esm --module es2015 --watch && tsc --watch"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "@mdi/js": "^5.3.45",
    "@mdi/react": "^1.4.0",
    "@types/react": "^16.9.41",
    "@types/styled-components": "4.1.1",
    "polished": "^3.6.5",
    "react-is": "^16.13.1",
    "styled-components": "4.1.1"
  }
}
