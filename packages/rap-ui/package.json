{
  "name": "@rap/ui",
  "version": "1.2.3",
  "description": "React styled component for easier, and faster user interface development",
  "author": "Inioluwa Sogelola <inioluwwa4.is@gmail.com>",
  "homepage": "https://github.com/nerdi-caveman/react-admin-panel#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "esm/index.js",
  "sideEffects": false,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "*.md",
    "esm",
    "src",
    "docs"
  ],
  "devDependencies": {
    "@types/react-redux": "^7.1.1",
    "@types/react-router": "^5.1.0",
    "@types/react-router-dom": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
