{
  "name": "react-admin-panel-lerna",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "typescript": "~3.7.2",
    "react-scripts": "3.4.1",
    "react": "^16.13.1"
  },
  "scripts": {
    "watch": "lerna run --parallel watch",
    "run:demo": "cd examples/demo && cross-env REACT_APP_DATA_PROVIDER=rest yarn -s start",
    "build:demo": "cd examples/demo && yarn build",
    "run:demo-watch": "concurrently \"yarn run watch\" \"yarn run run-demo\"",
    "test": "react-scripts test",
    "prettier": "prettier"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "lerna": "^3.22.1",
    "prettier": "^2.0.5"
  }
}
